*.c     text diff=c
*.cc    text diff=cpp
*.h     text diff=c

*.lo    binary
*.o     binary
*.obj   binary

*.gch   binary
*.pch   binary

*.so    binary
*.dll   binary

*.la    binary
*.a     binary
*.lib   binary

*.exe   binary
*.out   binary
*.app   binary

*.pl ident

*.pl text diff=perl
*.pm text diff=perl

*.py ident

*.pxd    text diff=python
*.py     text diff=python
*.py3    text diff=python
*.pyw    text diff=python
*.pyx    text diff=python
*.pyz    text diff=python

*.db     binary
*.p      binary
*.pkl    binary
*.pickle binary
*.pyc    binary
*.pyd    binary
*.pyo    binary

*.ipynb  text

*.vim text eol=lf
